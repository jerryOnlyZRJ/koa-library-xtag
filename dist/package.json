{
    "name": "jws2",
    "version": "1.0.0",
    "description": "Jerry's Web Scaffold",
    "main": "index.js",
    "scripts": {
        "start": "scripty",
        "build": "scripty",
        "test": "npm run test:unit",
        "start:dev": "scripty",
        "server:dev": "cross-env NODE_ENV=development gulp",
        "server:prod": "cross-env NODE_ENV=production gulp",
        "server:lint": "cross-env NODE_ENV=lint gulp",
        "client:dev": "webpack --mode development",
        "client:prod": "webpack --mode production --module-bind js=eslint-loader!babel-loader",
        "test:unit": "jest --coverage --forceExit",
        "test:e2e": "node ./tests/e2e.js",
        "tools:webpack-chart": "webpack --mode production --profile --json > stats.json",
        "tools:docs": "jsdoc ./src/server/**/*.js -d ./docs/jsdocs"
    },
    "scripty": {
        "parallel": true
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged",
            "pre-push": "npm run test"
        }
    },
    "author": "920997736@qq.com",
    "license": "ISC",
    "lint-staged": {
        "src/server/**/*.js": [
            "prettier --write",
            "eslint --fix",
            "git add"
        ]
    },
